{
  "name": "templito",
  "version": "0.0.11",
  "engines": {
    "node": ">=0.8.0"
  },
  "description": "Generates javascript files for underscore.js templates.",
  "main": "index.js",
  "directories": {
    "test": "./test",
    "bin": "./bin"
  },
  "bin": {
    "templito": "./bin/templito"
