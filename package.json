  "keywords": [
    "underscore",
    "underscore.js",
    "template",
    "underscore",
    "templates"
  ],
  "author": "Paul Harper <benekastah@gmail.com>",
  "license": "BSD",
  "dependencies": {
    "coffee-script": "~1.9.1",
    "optimist": "~0.4.0",
    "q": "^1.2.0"
  },
  "optionalDependencies": {
    "underscore": "~1.8.2",
    "node-watch": "~0.3.4"
  },
  "preferGlobal": true
}
